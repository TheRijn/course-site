<div id="content" class="container">

	<header class="page-header">
		<h1>Grading list</h1>
	</header>

	<table class="table table-bordered">
		<tr>
			<th>pset</th>
			<th>name</th>
			<th>id</th>
			<th>submit date</th>
			<th></th>
		</tr>
		
		<% @submits.each do |submit| %>
		<tr>
			<td><%= submit.pset.name %></td>
			<td><%= submit.user.name %></td>
			<td><%= submit.user.uvanetid %></td>
			<td><%= time_ago_in_words(submit.submitted_at) if submit.submitted_at %> ago</td>
			<td><%= submit_grade_link(submit) %></td>
		</tr>
		<% end %>
	</ul>
	
</div>
