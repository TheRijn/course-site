<% content_for :math do %>
	<script type="text/x-mathjax-config">
		MathJax.Hub.Config({
			showProcessingMessages: true,
			extensions: ["tex2jax.js"],
			AsciiMath: {
				decimalsign: ","
			}
		});
	</script>
	<%= mathjax_tag %>
<% end %>

<% content_for :footer do %>
	<% #= render 'layouts/footer' %>
<% end %>

<%= render template: "layouts/application" %>
