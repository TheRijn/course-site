<div class="container-fluid">
	<div class="row-fluid">

		<div class="tabbable">
			<ul class="nav nav-tabs">
				<li class="active"><a href="#profile" data-toggle="tab">Profile</a></li>
			</ul>

			<div class="tab-content">
				<section class="tab-pane markdown active" id="profile">
					<table class="table table-bordered">
						<tr><th>Name</th><th>Email</th><th>Last login</th></tr>
						<% @users.each do |user| %>
						<tr><td><%= user.name %></td><td><%= user.mail %><td><%= time_ago_in_words(user.updated_at) %> ago</td></tr>
						<% end %>
					</table>
				</section>
			</div>
		</div>

	</div>
</div>
