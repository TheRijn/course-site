<div id="content" class="container">

	<header class="page-header">
		<h1>Admins</h1>
	</header>

	<%= form_tag({ action: "admins" }) do %>
	<div class="form-group">
		<div class="col-10">
			<textarea id="inputAdmins" name="admins" class="form-control" style="height:10em;"><%= @admins %></textarea>
			<%= submit_tag 'Save', class:'btn' %>
		</div>
	</div>
	<% end %>

</div>
